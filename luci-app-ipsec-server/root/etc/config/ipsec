
config service 'ipsec'
	option enabled '0'
	option secret 'ipsec'
	option clientip '192.168.100.10/24'
        option clientdns '192.168.1.1'
		option useLanDHCP '1'
		option lanDHCPServer '192.168.1.1'
        option ikev2enabled '0'
        option routerDomain 'yourddnsname.example'

config users
	option enabled '1'
        option username 'sirpdboy'
	option password '123456'

